<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--  Report Window  -->
    <system:String x:Key="ReportWindow_AppGotError">App got an error</system:String>
    <system:String x:Key="ReportWindow_PleaseOpenIssue">Please open a new issue in</system:String>
    <system:String x:Key="ReportWindow_UploadLog">1. Upload this log file: {0}</system:String>
    <system:String x:Key="ReportWindow_CopyBelow">2. Copy exception messages below</system:String>

    <!--  Editor Mode  -->
    <system:String x:Key="EditorMode_Math">Math</system:String>
    <system:String x:Key="EditorMode_Text">Text</system:String>

    <!--  Copy Type  -->
    <system:String x:Key="CopyType_Image">Image</system:String>
    <system:String x:Key="CopyType_Latex">Latex</system:String>

</ResourceDictionary>